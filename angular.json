{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "angular-monorepo-demo-university",
  "projects": {
    "angular-monorepo-demo-university": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": false
        }
      },
      "root": "apps/angular-monorepo-demo-university",
      "sourceRoot": "apps/angular-monorepo-demo-university/src",
      "prefix": "demo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/angular-monorepo-demo-university",
            "index": "apps/angular-monorepo-demo-university/src/index.html",
            "main": "apps/angular-monorepo-demo-university/src/main.ts",
            "polyfills": "apps/angular-monorepo-demo-university/src/polyfills.ts",
            "tsConfig": "apps/angular-monorepo-demo-university/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/angular-monorepo-demo-university/src/favicon.ico",
              "apps/angular-monorepo-demo-university/src/assets"
            ],
            "styles": ["apps/angular-monorepo-demo-university/src/styles.css"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/angular-monorepo-demo-university/src/environments/environment.ts",
                  "with": "apps/angular-monorepo-demo-university/src/environments/environment.prod.ts"
                }
              ],
              "buildOptimizer": true,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false
            },
            "development": {
              "vendorChunk": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "angular-monorepo-demo-university:build:production"
            },
            "development": {
              "browserTarget": "angular-monorepo-demo-university:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-monorepo-demo-university:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/angular-monorepo-demo-university/src/test.ts",
            "polyfills": "apps/angular-monorepo-demo-university/src/polyfills.ts",
            "tsConfig": "apps/angular-monorepo-demo-university/tsconfig.spec.json",
            "karmaConfig": "apps/angular-monorepo-demo-university/karma.conf.js",
            "assets": [
              "apps/angular-monorepo-demo-university/src/favicon.ico",
              "apps/angular-monorepo-demo-university/src/assets"
            ],
            "styles": ["apps/angular-monorepo-demo-university/src/styles.css"],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-monorepo-demo-university:serve:production"
            },
            "development": {
              "devServerTarget": "angular-monorepo-demo-university:serve:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    },
    "angular-monorepo-demo-university-e2e": {
      "root": "apps/angular-monorepo-demo-university-e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/angular-monorepo-demo-university-e2e/protractor.conf.js"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-monorepo-demo-university:serve:production"
            },
            "development": {
              "devServerTarget": "angular-monorepo-demo-university:serve:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "apps/angular-monorepo-demo-university-e2e/tsconfig.json"
          }
        }
      }
    },
    "course-feature-course": {
      "projectType": "library",
      "root": "libs/course/feature-course",
      "sourceRoot": "libs/course/feature-course/src",
      "prefix": "angular-monorepo-demo-university",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/course/feature-course"],
          "options": {
            "jestConfig": "libs/course/feature-course/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/course/feature-course/src/**/*.ts",
              "libs/course/feature-course/src/**/*.html"
            ]
          }
        }
      }
    },
    "curriculum-feature-curriculum-list": {
      "projectType": "library",
      "root": "libs/curriculum/feature-curriculum-list",
      "sourceRoot": "libs/curriculum/feature-curriculum-list/src",
      "prefix": "angular-monorepo-demo-university",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/curriculum/feature-curriculum-list"],
          "options": {
            "jestConfig": "libs/curriculum/feature-curriculum-list/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/curriculum/feature-curriculum-list/src/**/*.ts",
              "libs/curriculum/feature-curriculum-list/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-ui": {
      "projectType": "library",
      "root": "libs/shared/ui",
      "sourceRoot": "libs/shared/ui/src",
      "prefix": "angular-monorepo-demo-university",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/ui"],
          "options": {
            "jestConfig": "libs/shared/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/ui/src/**/*.ts",
              "libs/shared/ui/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-util": {
      "projectType": "library",
      "root": "libs/shared/util",
      "sourceRoot": "libs/shared/util/src",
      "prefix": "angular-monorepo-demo-university",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/util"],
          "options": {
            "jestConfig": "libs/shared/util/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/util/src/**/*.ts",
              "libs/shared/util/src/**/*.html"
            ]
          }
        }
      }
    },
    "shared-model": {
      "projectType": "library",
      "root": "libs/shared/model",
      "sourceRoot": "libs/shared/model/src",
      "prefix": "angular-monorepo-demo-university",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/model"],
          "options": {
            "jestConfig": "libs/shared/model/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/model/src/**/*.ts",
              "libs/shared/model/src/**/*.html"
            ]
          }
        }
      }
    },
    "course-data-access": {
      "projectType": "library",
      "root": "libs/course/data-access",
      "sourceRoot": "libs/course/data-access/src",
      "prefix": "angular-monorepo-demo-university",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/course/data-access"],
          "options": {
            "jestConfig": "libs/course/data-access/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/course/data-access/src/**/*.ts",
              "libs/course/data-access/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "schematics": {
    "@nrwl/angular:application": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {}
  }
}
